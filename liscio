	   | "Lara"
	   | "Chiara"
	   | "Serena"
	   | "Lella"
	   | "Michela"
	   | "Lisa"
	   | "Zena"
	   | "Gabriella"
	   | "Marianna"
	   | "Alice"
	  
ei -> e " i Clan"
    | e " le Vibrazioni"
	| e " i Diapason"
	| e " i Totem"
	| e " gli Amici"
	| e " le Amiche"
	| e " i Nevada"
	| e " i Melody"
	| e " i Casanova"
	| e " i Colibrì"
	| e " i Ricordi"
	| e " gli Eureka"
	| e " i Sole"
	| "Glamour"
	| null
     
	  

